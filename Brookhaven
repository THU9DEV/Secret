Home:Seperator("Other")

Plr = {}


for i,v in pairs(game:GetService("Players"):GetChildren()) do
    table.insert(Plr,v.Name) 
end


local Player = Home:Dropdown("Dropdown",Plr,function(value)
	PlayerTP = value
end)

Home:Button("Refresh",function()
Player:Clear()
for i,v in pairs(game:GetService("Players"):GetChildren()) do
   table.insert(Plr,v.Name)
end
end)



